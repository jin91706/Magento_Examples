<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Cron:etc/crontab.xsd">
    <group id="default">
        <job name="vnecoms_order_num_reset_order_counter" instance="Vnecoms\CustomOrderNumber\Cron\ResetOrderNum" method="process">
            <config_path>crontab/default/jobs/vnecoms_order_num_reset_order_counter/schedule/cron_expr</config_path>
        </job>
        <job name="vnecoms_order_num_reset_invoice_counter" instance="Vnecoms\CustomOrderNumber\Cron\ResetInvoiceNum" method="process">
            <config_path>crontab/default/jobs/vnecoms_order_num_reset_invoice_counter/schedule/cron_expr</config_path>
        </job>
        <job name="vnecoms_order_num_reset_shipment_counter" instance="Vnecoms\CustomOrderNumber\Cron\ResetShipmentNum" method="process">
            <config_path>crontab/default/jobs/vnecoms_order_num_reset_shipment_counter/schedule/cron_expr</config_path>
        </job>
        <job name="vnecoms_order_num_reset_creditmemo_counter" instance="Vnecoms\CustomOrderNumber\Cron\ResetCreditmemoNum" method="process">
            <config_path>crontab/default/jobs/vnecoms_order_num_reset_creditmemo_counter/schedule/cron_expr</config_path>
        </job>
    </group>
</config>
